{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Débogueur Python : Fichier chat STL",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [
        "--svg",
        "${workspaceFolder}/svgs/le_chat.svg",
        "--size",
        "40",
        "--output",
        "${workspaceFolder}/stls/moule_chat.stl",
        "--keep-debug-files"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Débogueur Python : Fichier Anneau STL",
